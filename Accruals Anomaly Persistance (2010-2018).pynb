        "plt.ylabel('Mean Earnings')\n",
        "plt.plot(EarningsMean[\"High\"],label='High Accruals Portfolio')\n",
        "plt.plot(EarningsMean[\"Low\"],label='Low Accruals Portfolio')\n",
        "plt.legend()"
